{
  "canisters": {
    "mugc-agc-backend": {
      "candid": "src/mugc-agc-backend/mugc-agc-backend.did",
      "package": "mugc-agc-backend",
      "type": "rust",
      "assets": ["alexa.rpw"]
    },
    "mugc-agc-frontend": {
      "dependencies": [
        "mugc-agc-backend"
      ],
      "source": [
        "src/mugc-agc-frontend/dist"
      ],
      "type": "assets",
      "workspace": "mugc-agc-frontend"
    },
    "ic_oss_bucket": {
      "candid": "ic-oss/src/ic_oss_bucket/ic_oss_bucket.did",
      "declarations": {
        "node_compatibility": true
      },
      "package": "ic_oss_bucket",
      "optimize": "cycles",
      "type": "rust"
    },
    "ic_oss_cluster": {
      "candid": "ic-oss/src/ic_oss_cluster/ic_oss_cluster.did",
      "declarations": {
        "node_compatibility": true
      },
      "package": "ic_oss_cluster",
      "optimize": "cycles",
      "type": "rust"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    },
    "replica": {
      "subnet_type": "system"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943"
    }
  },
  "output_env_file": ".env",
  "version": 1
}